        <% include ./partials/keywords %>

        <% include ./partials/image_handling %>
        </div>
    </div>
    <footer class="footer">
      <div class="container">
        <span class="text-muted">TexToLatex Inc.</span>
        <span class="text-muted">Careers</span>
        <span class="text-muted">Locations</span>
        <span class="text-muted">Contact</span>
      </div>
    </footer>
  </body>
